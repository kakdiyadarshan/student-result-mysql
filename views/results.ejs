<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Studnet Result</title>
</head>
<body>

    <h1>Result </h1>

    <form method="post">
        <tr>
            <td>Std:</td>
            <select name="std">
                <% result.forEach(function(data) { %>
                <option value="<%= data.std %>"><%= data.std %></option>
                <% }); %>
            </select>
        </tr><br>
        <tr>
            <td>Div:</td>
            <td><select name="div">
                <% result1.forEach(function(data) { %>
                    <option value="<%= data.divi %>"><%= data.divi %></option>
                <% }); %>
            </select></td><br>
        </tr>
        <tr>
            <td>Roll No:</td>
            <td><input type="text" name="rno"></td><br>
        </tr>
        <tr>
            <td></td>
            <td><input type="submit" name="showres" value="Show Result"></td>
        </tr>
        
    </form>

    <table border="">
        <tr>
            <td>Id</td>
            <td>Std</td>
            <td>Div</td>
            <td>Roll No</td>
            <td>Name</td>
            <td>Sub 1</td>
            <td>Sub 2</td>
            <td>Sub 3</td>
            <td>Sub 4</td>
            <td>Total</td>
            <td>Per</td>
        </tr>
        <% result2.forEach(function(data) { %>
            <tr>
                <td><%= data.id %></td>
                <td><%= data.std %></td>
                <td><%= data.divi %></td>
                <td><%= data.rno %></td>
                <td><%= data.name %></td>
                <td><%= data.s1 %></td>
                <td><%= data.s2 %></td>
                <td><%= data.s3 %></td>
                <td><%= data.s4 %></td>
                <td><%= data.total %></td>
                <td><%= data.per %></td>
            </tr>
        <% }); %>
    </table>

    <h6><a href="/reslogout">logout</a></h6>
    
</body>
</html>